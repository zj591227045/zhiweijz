{
  "name": "zhiweijz",
  "version": "0.2.2",
  "description": "一个简单、纯粹、AI驱动的移动端记账工具。",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "server:start": "node dist/server/src/server.js",
    "server:dev": "nodemon",
    "server:build": "tsc",
    "server:lint": "eslint \"server/src/**/*.ts\" \"client/src/**/*.ts\" \"shared/**/*.ts\" --fix",
    "test:server": "jest --testPathPattern=server",
    "test:client": "jest --testPathPattern=client",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:full": "node scripts/run-full-tests.js",
    "test:backend-api": "node scripts/test-backend-api.js",
    "test:frontend": "node scripts/test-frontend.js",
    "test:e2e": "node scripts/test-e2e.js",
    "test:regression": "node scripts/test-regression.js",
    "test:health": "node scripts/test-health-check.js",
    "test:guide": "node scripts/test-environment-guide.js",
    "prisma:generate": "cd server && prisma generate",
    "prisma:migrate": "cd server && prisma migrate dev",
    "prisma:studio": "cd server && prisma studio",
    "version:show": "node scripts/update-version.js",
    "version:update": "node scripts/update-version.js",
    "android:build": "cd apps/android && ./build-release.sh",
    "db:enable-user-ai": "node scripts/database/enable-user-ai-services.js",
    "db:reset-password": "node scripts/database/reset-user-password.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.15.18",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "babel-jest": "^29.6.3",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.6.3",
    "nodemon": "^3.1.10",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "sharp": "^0.34.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@langchain/core": "^0.3.56",
    "@langchain/langgraph": "^0.2.72",
    "@langchain/openai": "^0.5.10",
    "@prisma/client": "^5.0.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-navigation/bottom-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/stack": "^7.1.1",
    "@tanstack/react-query": "^5.62.7",
    "@types/cors": "^2.8.18",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^5.0.1",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.17",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.2",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.3",
    "prisma": "^5.0.0",
    "react": "18.3.1",
