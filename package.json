    "react-native": "0.76.5",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-screens": "^4.1.0",
    "react-native-vector-icons": "^10.2.0",
    "recharts": "^2.15.3",
    "styled-components": "^6.1.18"
  }
}
