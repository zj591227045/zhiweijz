# 微信公众号配置示例
# 复制此文件为 .env 并填入真实的配置信息

# 微信公众号配置
WECHAT_APP_ID=
WECHAT_APP_SECRET=
WECHAT_TOKEN=your_wechat_token
WECHAT_ENCODING_AES_KEY=

# 数据库配置
DATABASE_URL=postgresql://username:password@localhost:5432/zhiweijz?schema=public

# JWT配置
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# 服务器配置
PORT=3000
NODE_ENV=development

# OpenAI配置（用于智能记账）
OPENAI_API_KEY=your_openai_api_key

# 邮件配置（可选）
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_email@example.com
SMTP_PASS=your_email_password


# 开发环境配置
  WECHAT_ENV=development
  WECHAT_DEV_APP_ID=your_test_appid
  WECHAT_DEV_APP_SECRET=your_test_secret
  WECHAT_DEV_TOKEN=your_test_token

  